  <![endif]-->
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="@Styles.Url("~/content/css")" rel="stylesheet"/>
    @RenderSection("head", required: false) @Html.Partial("_html5shiv")
    @* favicons and touch icons go here *@
    <style type="text/css">
        header
        {
            background-color: #212121;
            background-image: linear-gradient(#333333, #262626 95%, #0D0D0D);
            background-repeat: no-repeat;
            border-top: 3px solid #428BCA;
            height: 38px;
            line-height: 38px;
            vertical-align: middle;
            width: 100%;
        }
        header .nav .dropdown-toggle .caret
        {
            border-bottom-color: #FFF;
            border-top-color: #FFF;
        }
        
        header .nav > li > a
        {
            color: #FFF;
        }
        header .nav > li > a:hover, .nav > li > a:focus
        {
            background-color: transparent;
        }
        header .dropdown.open .dropdown-toggle
        {
            background-color: transparent;
        }
        header .nav .dropdown-toggle:hover .caret, .nav .dropdown-toggle:focus .caret
        {
            border-bottom-color: #FFF;
            border-top-color: #FFF;
        }
        
        
        
        #manu-top ul
        {
            margin-bottom: 0px;
        }
        #manu-top .nav .dropdown-toggle:focus .caret, #manu-top .nav .dropdown-toggle:hover .caret
        {
            border-bottom-color: #FFF;
            border-top-color: #FFF;
        }
        #manu-top > ul > li
        {
            background: none repeat scroll 0 0 #FAFAFA;
            border-radius: 2px 2px 0 0;
            float: left;
            margin-right: 1px;
            min-width: 95px;
            transition: all 0.2s ease 0s;
        }
        #manu-top ul li a
        {
            color: #404040;
            display: block;
            font-size: 13px;
            line-height: 18px;
            padding: 10px;
            text-align: left;
        }
        
        .selected, #manu-top li:hover
        {
            background: none repeat scroll 0 0 #428BCA !important;
            border-radius: 2px 2px 0 0;
            transition: all 0.2s ease 0s;
        }
        .selected a, #manu-top li a:hover
        {
            color: #FFF !important;
            background: none repeat scroll 0 0 #428BCA !important;
            border-radius: 2px 2px 0 0;
        }
        #manu-top .open .dropdown-toggle
        {
            background: none repeat scroll 0 0 #428BCA;
            border-radius: 2px 2px 0 0;
            color: #FFF;
        }
        
        
        
        
        
        
        
        
        
        
        
        
        .container-fluid
        {
            padding-left: 10px;
            padding-right: 10px;
        }
        .user-details
        {
            margin-bottom: 3px;
            margin-top: 10px;
        }
        .user-details img
        {
            float: left;
        }
        .welcome-text
        {
            color: #428BCA;
            font-size: 12px;
            padding-top: 5px;
            text-align: right;
        }
        
        
        .LeftMargin10
        {
            margin-left: 10px;
        }
        .mainContainer
        {
            background: none repeat scroll 0 0 #FAFAFA;
            border-radius: 0 3px 3px;
            border-top: 5px solid #428BCA;
            min-height: 570px;
            padding: 10px 15px;
        }
    </style>
</head>
  
<body>
    <header>
        <ul class="nav nav-pills pull-right">
            <li><a href="#">Home</a></li>
            <li class="dropdown"><a href="#" data-toggle="dropdown" class="dropdown-toggle">Dropdown
                <b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li><a href="#">Action</a></li>
                    <li><a href="#">Another action</a></li>
                    <li><a href="#">Something else here</a></li>
                    <li class="divider"></li>
                    <li><a href="#">Separated link</a></li>
                </ul>
            </li>
            <li><a href="#">Help</a></li>
            <li><a href="#">About us</a></li>
        </ul>
    </header>
    <div class="container-fluid">
        
        <div class="span12 LeftMargin10">
            <div class="row-fluid">
                <div id="manu-top">
                    <ul class="nav nav-pills">
                        <li class="selected"><a href="#">Dashboard</a> </li>
                        <li><a href="#">Reports</a></li>
                        <li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#">Forms
                            <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="#" data-original-title="">Basic Forms</a> </li>
                                <li><a href="#" data-original-title="">Extended Forms</a> </li>
                                <li><a href="#" data-original-title="">Form Wizards</a> </li>
                            </ul>
                        </li>
                        <li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#">Graphs
                            <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="#" data-original-title="">Flot Charts</a> </li>
                                <li><a href="#" data-original-title="">Google Charts</a> </li>
                                <li><a href="#" data-original-title="">Animated Charts</a> </li>
                            </ul>
                        </li>
                        <li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#">UI Lab
                            <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="#" data-original-title="">Clients List</a> </li>
                                <li><a href="#" data-original-title="">General Elements</a> </li>
                                <li><a href="#" data-original-title="">Messages</a> </li>
                                <li><a href="#" data-original-title="">Timeline</a> </li>
                                <li><a href="#" data-original-title="">Pricing Plans</a> </li>
                                <li><a href="#" data-original-title="">Grid Layout</a> </li>
                                <li><a href="#" data-original-title="">Buttons &amp; Icons</a> </li>
                                <li><a href="#" data-original-title="">Typography</a> </li>
                                <li><a href="#" data-original-title="">Blank Page</a> </li>
                            </ul>
                        </li>
                        <li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#">Tables
                            <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="#" data-original-title="">Static Tables</a> </li>
                                <li><a href="#" data-original-title="">Dynamic Tables</a> </li>
                            </ul>
                        </li>
                        <li><a href="#">Gallery</a> </li>
                        <li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#">Dropdown
                            <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="#" data-original-title="">Invoice</a> </li>
                                <li><a href="#" data-original-title="">Calendar</a> </li>
                                <li><a href="#" data-original-title="">Profile</a> </li>
                                <li><a href="#" data-original-title="">404 Error</a> </li>
                                <li><a href="#" data-original-title="">Faq</a> </li>
                                <li><a href="#" data-original-title="">Login</a> </li>
                            </ul>
                        </li>
                    </ul>
                    <div class="clearfix">
                    </div>
                </div>
            </div>
            <div class="row-fluid">
                @Scripts.Render("~/js") @RenderSection("Scripts", required: false)
                @RenderBody()
            </div>
        </div>
    </div>
    <footer>
    </footer>
    
</body>
</html>
