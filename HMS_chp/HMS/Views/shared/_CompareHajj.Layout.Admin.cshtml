                                </a>
                                   
                                
                                <ul class="dropdown-menu">
                                    <li>@Html.ActionLink("User Info", "Details", "UserAccounts", new { id = Session["UserId"].ToString() }, null)</li>
                                   
                                    <li>@Html.ActionLink("Logout", "LogOut", "cPanel", new { id = Session["UserId"].ToString() }, null)</li>
                                </ul>
                            </li>
                         
                                @*<li class="dropdown hideBorder">
                                    <a data-toggle="dropdown" class="dropdown-toggle" href="#"><span aria-hidden="true" class="glyphicon glyphicon-user"></span>@Session["UserName"].ToString() <span class="caret"></span></a>
