@using System.Web.Optimization
@using BootstrapSupport
@using NavigationRoutes
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="@Styles.Url("~/content/css")" rel="stylesheet" />
    @RenderSection("head", required: false) @Html.Partial("_html5shiv")
    @* favicons and touch icons go here *@
    <style type="text/css">
        .user-details {
            margin-left: 5px; /*
            margin-bottom: 3px;
            margin-top: 10px;
            */
        }

            .user-details img {
                float: left;
                margin-top: 5px;
                width: 40px;
            }

        .welcome-text {
            border: 1px solid #DDDDDD;
            color: #428BCA;
            font-size: 1em;
            padding-right: 10px;
            text-align: right;
            height: 45px;
        }


        #manu-top > ul > li {
            background: none repeat scroll 0 0 #FAFAFA;
            border-radius: 2px 2px 0 0;
            float: left;
            margin-right: 1px;
            transition: all 0.2s ease 0s;
        }

        #manu-top ul li a {
            color: #404040;
            display: block;
            font-size: 1em;
            line-height: 18px;
            padding: 8px 7px;
            text-align: left;
        }

        .containerLeftMargin {
            margin-left: 5px !important;
           
        }

        .container {
            width: 95%;
            height:auto;
        }
        
    </style>
</head>
