@model HMS.Entities.Models.ClientInformation
@{
    ViewBag.Title = "ClientInfoCreate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@section breadcrumb
{
    <li><a href="~/Dashboard/index">Dashboard</a></li>
    <li><a href="~/User/">Package</a></li>   
    <li class="active">@ViewBag.Title</li>
}

<script type="text/javascript">
    $(document).ready(function () {
        $("#page-leftbar li").removeClass("active");
        $("#liAdmin").addClass("active open");
        $('#liAdmin .acc-menu').css('display', 'block');
        $('#liAdmin .acc-menu').collapse('show');
    });
</script>



@section title
{
    @ViewBag.Title
}
@Html.ActionLink("Back to List", "Index")
<hr />

@using (Html.BeginForm("ClientInfoCreate","ClientInformation", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()

    <div class="row">

        @Html.ValidationSummary(true)
        <div class="form-group">
            @Html.LabelFor(model => model.Address, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.Tellandline, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Tellandline)
            </div>

        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.TelMobile, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.TelMobile, new { @class = "form-control" })
              
                @Html.ValidationMessageFor(model => model.TelMobile)
            </div>

        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Email, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.Email, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Email)
            </div>

        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Website, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.Website, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Website)
            </div>

        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.YearEstablished, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.YearEstablished, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.YearEstablished)
            </div>

        </div>

        <div class="form-group">
            <label for="checkbox" class="col-sm-3 control-label"></label>
            <div class="col-sm-6">
                <div class="checkbox block">
                    <label>
                        @Html.CheckBoxFor(model => model.IsApprovedbySaudiHajjMinistry) @Html.LabelFor(model => model.IsApprovedbySaudiHajjMinistry)
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.MinistryApprovalNumber, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.MinistryApprovalNumber, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.MinistryApprovalNumber)
            </div>

        </div>
        <div class="form-group">
            <label for="checkbox" class="col-sm-3 control-label"></label>
            <div class="col-sm-6">
                <div class="checkbox block">
                    <label>
                        @Html.CheckBoxFor(model => model.ATOLApproved) @Html.LabelFor(model => model.ATOLApproved)
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.ATOLLicense, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.ATOLLicense, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.ATOLLicense)
            </div>

        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.IATANo, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.IATANo, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.IATANo)
            </div>

        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.CompanyRegistrationNumber, new { @class = "col-sm-3 control-label" })
            <div class="col-sm-6">
                @Html.TextBoxFor(model => model.CompanyRegistrationNumber, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.CompanyRegistrationNumber)
            </div>

        </div>
    </div>
    <div class="row">
        <div class="col-sm-6 col-sm-offset-3">
            <div class="btn-toolbar">
                <input type="submit" value="Create" class="btn-primary btn" onclick=" return AddConfirm()" />
            </div>
        </div>
    </div>
}

@section Scripts {
@Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
}
